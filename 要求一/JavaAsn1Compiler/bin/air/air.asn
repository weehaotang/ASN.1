AirCondition DEFINITIONS IMPLICIT TAGS ::=

BEGIN


AirRecord2 ::= SEQUENCE
{
	hot 			[0] 		 BOOLEAN OPTIONAL,
	degree			[1] EXPLICIT INTEGER,
	cloud						 Cloud2,
	location  	[PRIVATE 1]		 City2,
	time		[APPLICATION 8]  Minutes2
}

Cloud2 ::=  SET {
	speed			Speed2,
	isBlue			BOOLEAN,
	parameters	[0]	IntSeq
}

IntSeq ::= SEQUENCE OF INTEGER

Speed2  ::=  OCTET STRING

City2 ::= ENUMERATED {
	istanbul 	(1),
	ankara 		(2),
	izmir	(5)
}

Minutes2 ::= INTEGER {min(0), max(59)}

